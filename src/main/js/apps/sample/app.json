{
    "properties": {
        "amdPackages": "apprt@^3.4.0"
    },
    "load": {
        "bundleLocations": ["localbundles", "bundles"],
        "allowedBundles": [
            "system@^3.4.0",
            "splashscreen@^3.4.0",
            "map@^3.4.0",
            "themes@^3.4.0",
            "templates@^3.4.0",
            "templatelayout@^3.4.0",
            "windowmanager@^3.4.0",
            "toolset@^3.4.0",
            "toolrules@^3.4.0",
            "overviewmap@^3.4.0",
            "coordinateviewer@^3.4.0",
            "dn_layoutBundleStarter"
        ],
        "skipBundles" : [],
        "require" : ["${app}.app"],
        "i18n": ["bundle"]
    },
    "bundles" : {
        "map" : {
            "MapState" : {
                "initialExtent":{
                    "xmin" : -1100000,
                    "ymin" : 4000000,
                    "xmax" : 3500000,
                    "ymax" : 8800000,
                    "spatialReference": {
                        "wkid": 3857
                    }
                }
            },
            "MapModelFactory" : {
                "_configData" : {
                    "maps" :[
                        {
                            "id": "default",
                            "glass_pane" : [],
                            "operationalLayer" : [],
                            "baseLayer" : [
                                {
                                    "title": "${service.streets}",
                                    "service": "streets",
                                    "enabled" : true
                                },
                                {
                                    "title": "${service.topo}",
                                    "service": "worldTopo",
                                    "enabled" : false
                                }
                            ]
                        }
                    ]
                }
            },
            "MappingResourceRegistryFactory" : {
                "_knownServices" :{
                    "services": [
                        {
                            "id":"worldTopo",
                            "type":"AGS_TILED",
                            "url":"http://services.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer"
                        },
                        {
                            "id":"streets",
                            "type":"AGS_TILED",
                            "url":"http://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer"
                        }
                    ]
                }
            }
        }
    }
}
